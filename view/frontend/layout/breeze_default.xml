<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <remove src="Amasty_HidePrice::js/fancyambox/jquery.fancyambox.min.css"/>
    </head>
    <body>
        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="bundles" xsi:type="array">
                    <item name="dynamic" xsi:type="array">
                        <item name="items" xsi:type="array">
                            <item name="Amasty_HidePrice/*" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_HidePrice/*</item>
                            </item>
                            <item name="amasty-fancyambox" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_BreezeAmastyHidePrice/js/fancyambox</item>
                            </item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
